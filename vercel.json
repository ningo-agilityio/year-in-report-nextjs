
{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "rewrites": [
    { "source": "/api/renderers", "destination": "/api/renderers.ts" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "DELETE, POST, GET, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization, X-Requested-With, x-copilotkit-runtime-client-gql-version" }
      ]
    }
  ]
}